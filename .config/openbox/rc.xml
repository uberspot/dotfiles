<?xml version="1.0"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
  <resistance>
    <strength>10</strength>
    <screen_edge_strength>20</screen_edge_strength>
  </resistance>
  <focus>
    <focusNew>yes</focusNew>
    <followMouse>no</followMouse>
    <focusLast>yes</focusLast>
    <underMouse>no</underMouse>
    <focusDelay>100</focusDelay>
    <raiseOnFocus>no</raiseOnFocus>
  </focus>
  <placement>
    <policy>Smart</policy>
    <center>no</center>
    <monitor>Active</monitor>
    <primaryMonitor>Active</primaryMonitor>
  </placement>
  <theme>
    <name>Onyx-black</name>
    <titleLayout>NLIMC</titleLayout>
    <keepBorder>yes</keepBorder>
    <animateIconify>no</animateIconify>
    <font place="ActiveWindow">
      <name>Standard Symbols L</name>
      <size>8</size>
      <weight>Bold</weight>
      <slant>Normal</slant>
    </font>
    <font place="InactiveWindow">
      <name>Standard Symbols L</name>
      <size>8</size>
      <weight>Bold</weight>
      <slant>Normal</slant>
    </font>
    <font place="MenuHeader">
      <name>Standard Symbols L</name>
      <size>9</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="MenuItem">
      <name>Standard Symbols L</name>
      <size>9</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="ActiveOnScreenDisplay">
      <name>Standard Symbols L</name>
      <size>8</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="InactiveOnScreenDisplay">
      <name>Standard Symbols L</name>
      <size>8</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
  </theme>
  <desktops>
    <number>2</number>
    <firstdesk>1</firstdesk>
    <names>
      <name>Main</name>
    </names>
    <popupTime>0</popupTime>
  </desktops>
  <resize>
    <drawContents>no</drawContents>
    <popupShow>Nonpixel</popupShow>
    <popupPosition>Center</popupPosition>
    <popupFixedPosition>
      <x>10</x>
      <y>10</y>
    </popupFixedPosition>
  </resize>
  <margins>
    <top>0</top>
    <bottom>26</bottom>
    <left>0</left>
    <right>0</right>
  </margins>
  <dock>
    <position>TopLeft</position>
    <floatingX>0</floatingX>
    <floatingY>0</floatingY>
    <noStrut>no</noStrut>
    <stacking>Below</stacking>
    <direction>Vertical</direction>
    <autoHide>no</autoHide>
    <hideDelay>200</hideDelay>
    <showDelay>200</showDelay>
    <moveButton>Middle</moveButton>
  </dock>
  <keyboard>
    <chainQuitKey>C-g</chainQuitKey>
    <keybind key="XF86PowerOff">
      <action name="Execute">
        <command>oblogout</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>oblogout</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-d">
      <action name="ToggleShowDesktop"/>
    </keybind>
    <keybind key="A-F4">
      <action name="Close"/>
    </keybind>
    <keybind key="S-W-w">
      <action name="Close"/>
    </keybind>
    <keybind key="S-W-e">
      <action name="Execute">
        <command>gmrun</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>gmrun</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-l">
      <action name="Lower"/>
      <action name="FocusToBottom"/>
      <action name="Unfocus"/>
    </keybind>
    <keybind key="S-A-Tab">
      <action name="PreviousWindow">
        <allDesktops>yes</allDesktops>
      </action>
    </keybind>
    <keybind key="A-Tab">
      <action name="NextWindow">
        <allDesktops>yes</allDesktops>
      </action>
    </keybind>
    <keybind key="W-f">
      <action name="Execute">
        <command>thunar</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>File Manager</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-w">
      <action name="Execute">
        <command>firefox</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Minefield Web Browser</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-x">
      <action name="Execute">
        <command>chromium</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>chromium</wmclass>
          <name>chromium</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-v">
      <action name="Execute">
        <command>vivaldi-stable</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>vivaldi</wmclass>
          <name>vivaldi</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-h">
      <action name="Execute">
          <command>HipChat4</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Hipchat</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-p">
      <action name="Execute">
        <command>pidgin</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Pidgin</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-t">
      <action name="Execute">
        <command>terminator --geometry=1150x700</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>terminator</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="S-W-q">
      <action name="Execute">
        <command>xkill</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Kill an application</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-n">
      <action name="Execute">
        <command>/usr/bin/telegram-desktop</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>telegram</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="S-W-r">
      <action name="Reconfigure"/>
    </keybind>
    <keybind key="W-m">
      <action name="Execute">
        <command>firejail --seccomp thunderbird</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Thunderbird</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-e">
      <action name="Execute">
        <command>subl</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>sublime</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="S-W-l">
      <action name="Execute">
        <command>slock</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>lock</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-s">
      <action name="Execute">
        <command>skype</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Skype</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="W-Tab">
      <action name="DesktopNext"/>
    </keybind>
    <keybind key="S-W-Tab">
      <action name="DesktopPrevious"/>
    </keybind>
    <keybind key="W-space">
      <action name="ShowMenu">
        <menu>root-menu</menu>
      </action>
    </keybind>
    <keybind key="S-W-Right">
      <action name="SendToDesktopNext"/>
    </keybind>
    <keybind key="S-W-Left">
      <action name="SendToDesktopPrevious"/>
    </keybind>
    <!-- Vertical tiling -->
    <keybind key="S-W-v">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
      </action>
      <action name="MaximizeVert"/>
      <action name="MoveResizeTo">
        <x>0</x>
        <y>0</y>
      </action>
      <action name="NextWindow">
        <interactive>no</interactive>
        <dialog>none</dialog>
        <finalactions>
          <action name="UnmaximizeFull"/>
          <action name="MoveResizeTo">
            <width>50%</width>
          </action>
          <action name="MaximizeVert"/>
          <action name="MoveResizeTo">
            <x>-0</x>
            <y>0</y>
          </action>
        </finalactions>
      </action>
    </keybind>
    <!-- Horizontal tiling -->
    <keybind key="S-W-h">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <height>50%</height>
      </action>
      <action name="MaximizeHorz"/>
      <action name="MoveResizeTo">
        <x>0</x>
        <y>0</y>
      </action>
      <action name="NextWindow">
        <interactive>no</interactive>
        <dialog>none</dialog>
        <finalactions>
          <action name="UnmaximizeFull"/>
          <action name="MoveResizeTo">
            <height>50%</height>
          </action>
          <action name="MaximizeHorz"/>
          <action name="MoveResizeTo">
            <x>0</x>
            <y>-0</y>
          </action>
        </finalactions>
      </action>
    </keybind>
    <!-- Restore window dimensions -->
    <keybind key="S-W-n">
      <action name="UnmaximizeFull"/>
      <action name="NextWindow">
        <interactive>no</interactive>
        <dialog>none</dialog>
        <finalactions>
          <action name="UnmaximizeFull"/>
        </finalactions>
      </action>
    </keybind>
    <keybind key="W-a">
      <action name="ToggleMaximizeFull"/>
    </keybind>
    <keybind key="W-Right">
      <action name="MoveToEdgeEast"/>
    </keybind>
    <keybind key="W-Up">
      <action name="MoveToEdgeNorth"/>
    </keybind>
    <keybind key="W-Left">
      <action name="MoveToEdgeWest"/>
    </keybind>
    <keybind key="W-Down">
      <action name="MoveToEdgeSouth"/>
    </keybind>
    <keybind key="W-0">
      <action name="Execute">
        <command>scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ '</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>ScrotSelected</name>
        </startupnotify>
      </action>
    </keybind>
    <keybind key="A-W-Up">
      <action name="GrowToEdgeNorth"/>
    </keybind>
    <keybind key="A-W-Right">
      <action name="GrowToEdgeEast"/>
    </keybind>
    <keybind key="A-W-Down">
      <action name="GrowToEdgeSouth"/>
    </keybind>
    <keybind key="A-W-Left">
      <action name="GrowToEdgeWest"/>
    </keybind>
    <keybind key="S-W-c">
      <action name="MoveToCenter"/>
    </keybind>
    <keybind key="XF86AudioRaiseVolume">
      <action name="Execute">
        <command>amixer set Master 5%+ unmute</command>
      </action>
    </keybind>
    <keybind key="XF86AudioLowerVolume">
      <action name="Execute">
        <command>amixer set Master 5%- unmute</command>
      </action>
    </keybind>
    <keybind key="XF86AudioMute">
      <action name="Execute">
        <command>pulseaudio-ctl mute</command>
      </action>
  </keybind>
  <!-- Super+PageUp -->
    <keybind key="W-Prior">
      <action name="Execute">
        <command>amixer set Master 5%+ unmute</command>
      </action>
  </keybind>
  <!-- Super+PageDown -->
    <keybind key="W-Next">
      <action name="Execute">
        <command>amixer set Master 5%- unmute</command>
      </action>
    </keybind>
    <keybind key="W-BackSpace">
      <action name="Execute">
        <command>pulseaudio-ctl mute</command>
      </action>
    </keybind>
    <keybind key="XF86ScreenSaver">
      <action name="Execute">
        <command>slock</command>
      </action>
    </keybind>
    <keybind key="XF86Sleep">
      <action name="Execute">
        <command>sudo pm-suspend</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessUp">
      <action name="Execute">
        <command>xbacklight +5</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessDown">
      <action name="Execute">
        <command>xbacklight -5</command>
      </action>
    </keybind>
    <keybind key="XF86Display">
      <action name="Execute">
        <command>~/.extra/display.sh</command>
        <startupnotify>
          <enabled>yes</enabled>
          <name>Enforce screen layout</name>
        </startupnotify>
      </action>
    </keybind>
  </keyboard>
  <mouse>
    <dragThreshold>12</dragThreshold>
    <!-- number of pixels the mouse must move before a drag begins -->
    <doubleClickTime>200</doubleClickTime>
    <!-- in milliseconds (1000 = 1 second) -->
    <screenEdgeWarpTime>250</screenEdgeWarpTime>
    <!-- Time before changing desktops when the pointer touches the edge of the
           screen while moving a window, in milliseconds (1000 = 1 second).
           Set this to 0 to disable warping -->
    <context name="Frame">
      <mousebind action="Press" button="A-Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Click" button="A-Left">
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Click" button="S-W-Down">
        <action name="SendToDesktopPrevious"/>
      </mousebind>
      <mousebind action="Click" button="S-W-Up">
        <action name="SendToDesktopNext"/>
      </mousebind>
      <mousebind action="Click" button="W-Down">
        <action name="DesktopPrevious"/>
      </mousebind>
      <mousebind action="Click" button="W-Up">
        <action name="DesktopNext"/>
      </mousebind>
      <mousebind action="Drag" button="A-Left">
        <action name="Move"/>
      </mousebind>
      <mousebind action="Press" button="A-Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Drag" button="A-Right">
        <action name="Resize"/>
      </mousebind>
      <!-- W + Middle click to grow a window to the edges of windows surrounding it. -->
      <mousebind button="W-Middle" action="Press">
        <action name="GrowToEdgeNorth"/>
        <action name="GrowToEdgeSouth"/>
        <action name="GrowToEdgeEast"/>
        <action name="GrowToEdgeWest"/>
      </mousebind>
    </context>
    <context name="Titlebar">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Move"/>
      </mousebind>
      <mousebind action="DoubleClick" button="Left">
        <action name="ToggleMaximizeFull"/>
      </mousebind>
      <mousebind action="Press" button="Middle">
        <action name="Lower"/>
        <action name="FocusToBottom"/>
        <action name="Unfocus"/>
      </mousebind>
      <mousebind action="Click" button="Up">
        <action name="Shade"/>
        <action name="FocusToBottom"/>
        <action name="Unfocus"/>
        <action name="Lower"/>
      </mousebind>
      <mousebind action="Click" button="Down">
        <action name="Unshade"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="Top">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize">
          <edge>top</edge>
        </action>
      </mousebind>
    </context>
    <context name="Left">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize">
          <edge>left</edge>
        </action>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="Right">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize">
          <edge>right</edge>
        </action>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="Bottom">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize">
          <edge>bottom</edge>
        </action>
      </mousebind>
      <mousebind action="Press" button="Middle">
        <action name="Lower"/>
        <action name="FocusToBottom"/>
        <action name="Unfocus"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="BLCorner">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize"/>
      </mousebind>
    </context>
    <context name="BRCorner">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize"/>
      </mousebind>
    </context>
    <context name="TLCorner">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize"/>
      </mousebind>
    </context>
    <context name="TRCorner">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Drag" button="Left">
        <action name="Resize"/>
      </mousebind>
    </context>
    <context name="Client">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Press" button="Middle">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
    </context>
    <context name="Icon">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="AllDesktops">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="ToggleOmnipresent"/>
      </mousebind>
    </context>
    <context name="Shade">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="ToggleShade"/>
      </mousebind>
    </context>
    <context name="Iconify">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="Iconify"/>
      </mousebind>
    </context>
    <context name="Maximize">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Press" button="Middle">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="ToggleMaximizeFull"/>
      </mousebind>
      <mousebind action="Click" button="Middle">
        <action name="ToggleMaximizeVert"/>
      </mousebind>
      <mousebind action="Click" button="Right">
        <action name="ToggleMaximizeHorz"/>
      </mousebind>
    </context>
    <context name="Close">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="Close"/>
      </mousebind>
    </context>
    <context name="Desktop">
      <mousebind action="Click" button="W-Up">
        <action name="DesktopPrevious"/>
      </mousebind>
      <mousebind action="Click" button="W-Down">
        <action name="DesktopNext"/>
      </mousebind>
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
    </context>
    <context name="Root">
      <!-- Menus -->
      <mousebind action="Press" button="Middle">
        <action name="Close"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="ShowMenu">
          <menu>root-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="MoveResize">
      <mousebind action="Click" button="Down">
        <action name="Iconify"/>
      </mousebind>
    </context>
  </mouse>
  <menu>
    <!-- default menu file (or custom one in $HOME/.config/openbox/) -->
    <file>menu.xml</file>
    <hideDelay>200</hideDelay>
    <middle>yes</middle>
    <submenuShowDelay>0</submenuShowDelay>
    <submenuHideDelay>0</submenuHideDelay>
    <applicationIcons>yes</applicationIcons>
    <manageDesktops>yes</manageDesktops>
    <showIcons>yes</showIcons>
  </menu>
  <applications>
    <application class="*">
      <decor>no</decor>
      <position>
        <monitor>1</monitor>
      </position>
      <focus>yes</focus>
      <maximized>no</maximized>
    </application>
    <!-- Force terminals to appear on top and on the right of the conky -->
    <application name="terminator">
      <desktop>1</desktop>
      <position force="yes">
        <x>210</x>
        <y>0</y>
      </position>
      <maximized>Vertical</maximized>
    </application>
    <application name="Telegram">
      <desktop>2</desktop>
    </application>
    <application class="Thunderbird">
      <desktop>1</desktop>
      <position force="yes">
        <x>210</x>
        <y>0</y>
      </position>
      <maximized>Vertical</maximized>
    </application>
    <application name="Pidgin">
      <desktop>2</desktop>
    </application>
    <application name="hipchat">
      <desktop>2</desktop>
    </application>
    <application class="Firefox">
      <desktop>1</desktop>
    </application>
  </applications>
